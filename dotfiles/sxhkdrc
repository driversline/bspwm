  alacritty
super + q
  firefox
super + d
  flameshot gui
super + l
  xset dpms force off
super + n
  alacritty -e ncmpcpp
super + c
  alacritty -e cava
super + o
  bspc wm -r
super + p
  pkill -USR1 -x sxhkd
super + x
  bspc node -c
super + Left
  bspc desktop -f prev
super + Right
  bspc desktop -f next
ctrl + Up
  bspc node -s north
ctrl + Down
  bspc node -s south
ctrl + Left
  bspc node -s west
ctrl + Right
  bspc node -s east
super + alt + Left
  bspc node -d prev
super + alt + Right
  bspc node -d next
super + Prior
  pactl set-sink-volume @DEFAULT_SINK@ +5%
super + Next
  pactl set-sink-volume @DEFAULT_SINK@ -5%
super + shift + Prior
  mpc prev
super + shift + Next
  mpc next
super + Return
  mpc toggle
super + shift + k
  mpc repeat on
super + shift + j
  mpc repeat off
super + a
  bspc node -t {tiled,floating}
